@{
    ViewBag.Title = "student";
}

<div class="dashboard">
    <div class="wrapper">
        <div style="color:#0000FF">
            <h3>Student Information</h3>
            <form action="create_project">
                <table align="center">
                    <tr><td>First name: <input type="text" name="fname" required></td></tr>
                    <tr><td>Last name: <input type="text" name="lname" required></td></tr>
                    <tr><td>Course ID: <input type="text" name="course_id" required></td></tr>
                    <tr><td>Project Name: <input type="text" name="project_name" required></td></tr>
                    <tr><td>Semester: <input type="text" name="semester" required></td></tr>
                    <tr><td><input type="submit" value="Submit"></td></tr>
                </table>
            </form>
            <a href="/Dashboard">Back to Dashboard</a> 
        </div>
        </div>
    </div>

