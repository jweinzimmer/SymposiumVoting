@{
    ViewBag.Title = "attendee";
}
<div class="dashboard">
    <div class="wrapper">
        <div style="color:#0000FF", class="attendee">
            <h3>Attendee Information</h3>
            <form action="create_attendee">
                <table align="center">
                    <tr><td>Email: <input type="text" name="email" required></td></tr>
                    <tr><td>First Name: <input type="text" name="fname" required></td></tr>
                    <tr><td>Last name: <input type="text" name="lname" required></td></tr>
                    <tr><td><input type="submit" value="Submit" required></td></tr>
                </table>
            </form>
            <a href="/Dashboard">Back to Dashboard</a> 
        </div>
    </div>
</div>


